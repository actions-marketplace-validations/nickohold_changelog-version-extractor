{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug: Local",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/lib/index.js",
      "cwd": "${workspaceFolder}",
      "args": [
        "start:local"
      ],
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "start:local",
        "--",
        "--inspect"
      ],
      "sourceMaps": true,
      "restart": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}